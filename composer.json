{
  "name": "jamisonbryant/cakephp-har-recorder",
  "description": "CakePHP 5 plugin that records HTTP requests and responses as HAR files.",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "cakephp/cakephp": "^5.0",
    "psr/http-message": "^2.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "JamisonBryant\\CakephpHarRecorder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JamisonBryant\\CakephpHarRecorder\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "cakephp-plugin": {
      "name": "CakephpHarRecorder"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
